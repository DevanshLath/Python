print("Welcome to the Calculator v2")
print(" ")
print(" ")

num1 = float(input("Enter Number 1: "))
num2 = float(input("Enter Number 2: "))
op = input("Enter Operator(+,-,*,/): ")

if op == "+":
    print(num1 + num2)
elif op == "-":
    print(num1 - num2)
elif op == "*":
    print(num1 * num2)
elif op == "/":
    print(num1 / num2)
else:
    print("Invalid!!")
